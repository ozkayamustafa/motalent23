MODULE user_command_0101 INPUT.
  CASE sy-ucomm.
    WHEN '&OK'.
        go_main->select_logo( ).
        LEAVE TO SCREEN 0.
    WHEN '&CANCEL'.
        LEAVE TO SCREEN 0.
    WHEN OTHERS.
  ENDCASE.
ENDMODULE.
